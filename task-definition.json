{
  "family": "simple-crawler-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::593793035927:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "simple-crawler-container",
      "image": "593793035927.dkr.ecr.us-west-1.amazonaws.com/simple-crawler:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}